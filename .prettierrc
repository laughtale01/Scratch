{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.java",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.md", "*.markdown"],
      "options": {
        "proseWrap": "preserve"
      }
    }
  ]
}